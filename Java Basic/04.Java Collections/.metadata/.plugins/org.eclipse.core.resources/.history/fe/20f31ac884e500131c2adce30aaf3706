import java.util.Scanner;


public class ThreeLargestNums {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int n=input.nextInt(),firstM,secondM,thirdM;
		int[] arr=new int[n],arrM=new int[3];
		arr[0]=input.nextInt();
		arrM[0]=arrM[1]=arrM[2]=arr[0];
		for (int i = 1; i <n; i++) {
			arr[i]=input.nextInt();
			if(arr[i]>arrM[0]) arrM[0]=arr[i];
			else if(arr[i]>arrM[1])arrM[1]=arr[i];
			else if(arr[i]>arrM[2])arrM[2]=arr[i];			
		}
		if(n<3)
		for (int i = 0; i <n; i++) {
			System.out.println(arr[i]);
		}
		else 
			for (int i = 0; i <3; i++) {
				System.out.println(arr[i]);
			}
	}

}
